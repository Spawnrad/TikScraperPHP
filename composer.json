{
    "name": "pablouser1/tikscraper",
    "description": "Get data from TikTok API",
    "type": "library",
    "license": "MIT",
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        }
    },
    "authors": [
        {
            "name": "Pablo Ferreiro"
        }
    ],
    "autoload": {
        "psr-4": {
            "TikScraper\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4|^8.0",
        "ext-curl": "*",
        "ext-xml": "*",
        "ext-json": "*",
        "php-webdriver/webdriver": "^1.12",
        "sapistudio/seleniumstealth": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
